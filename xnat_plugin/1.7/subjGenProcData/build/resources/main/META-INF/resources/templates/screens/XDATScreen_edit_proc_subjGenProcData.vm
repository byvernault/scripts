#if ($data.message)
<font color="red" size="3">$data.message</font>
#end
<p>
<form ID="form1" name="form1" method="post" action="$link.setAction("ModifySubjectAssessorData")">
<input type="hidden" name="project" value="$!{project}" >
#if($vr)
	<font color="red">Invalid parameters:<BR>$vr.toHTML()</font>
<HR>
#end
<script src="$content.getURI("scripts/omUtils.js")"></script>
<TABLE width="100%">
	<TR>
		<TD>
			<table width="100%">
				<TR>
					<TD align="left" valign="middle">
						<DIV class="edit_title">Subject Processor Details</DIV>
					</TD>
				</TR>
			</TABLE>
		</TD>
	</TR>
	<TR>
		<TD>
			<TABLE width="100%">
				<TR>
					<TD valign="top">
						<TABLE>
							<TR>
								<TD colspan='2'>
									<HR>
									#parse("/screens/xnat_edit_subjectData.vm")
									<HR>
								</TD>
							</TR>
							<TR>
								<TD colspan="2">#parse("/screens/EditProjectSpecificFields.vm")</TD>
							</TR>
                            <TR>
                            	<TD align="left" valign="top">
                        			<!-- BEGIN proc:subjGenProcData/validation -->
                                 	<TABLE>
                                        <TR><TH align="left"><BR><font face="$ui.sansSerifFonts" size="2">Processor QC</font></TH></TR>
                                        <TR>
                                        	<TD align="left" valign="top">
                                            	<TABLE>
                                                    <TR><TD>Status</TD><TD>#cndaSelectBoxSimple("proc:subjGenProcData/validation/status" $item $qcValues $vr)</TD></TR>
													<TR><TD>Method</TD><TD>#xdatStringBox("proc:subjGenProcData/validation/method" $item "" $vr)</TD></TR>
                                                    <TR><TD>Notes</TD><TD>#xdatTextArea("proc:subjGenProcData/validation/notes" $item "" $vr 10 30)</TD></TR>
													#xdatHiddenBox("proc:subjGenProcData/validation/xnat_validationData_id" $item "")
