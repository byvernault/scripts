													#xdatHiddenBox("proc:subjGenProcData/validation/validated_by" "" $user.username)
													#xdatHiddenBox("proc:subjGenProcData/validation/date" $item "")
                                                </TABLE>
                                            </TD>
                                        </TR>
                                	</TABLE>
                        			<!-- END proc:subjGenProcData/validation -->
                                </TD>
                             </TR>
					    </TABLE>
					</TD>
				</TR>
			</TABLE>
		</TD>
	</TR>
	<TR>
		<TD>
			#xdatEditProps($item $edit_screen)
			<!--input type="button" ONCLICK="validateForm()"  name="eventSubmit_doInsert" value="Submit"/!-->
			<TR><TD COLSPAN=2 ALIGN=left><button id="submit_qa">Submit</button></TD></TR>
		</TD>
	</TR>
</TABLE>
</form>

<script type="text/javascript">
	function setQCDate(){
		// Set the date
		document.getElementById('proc:subjGenProcData/validation/date').value = new Date().toISOString().slice(0,10);
	}

	document.getElementById('submit_qa').onclick=function(){
		setQCDate();
		document.getElementById("form1").submit();
	}


</script>
