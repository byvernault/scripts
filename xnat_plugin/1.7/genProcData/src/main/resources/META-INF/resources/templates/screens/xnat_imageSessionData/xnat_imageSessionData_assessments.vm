        obja.categories["assessors"].push({id:"$assessor.getId()",label:"$!assessor.getLabel() "});
      #end
    #end

	window.pviewer=new FileViewerProcessing(obja);
</script>
<script type="text/javascript">
	var objp=new Object();
    objp.uri=serverRoot + "/REST/experiments/$!om.getId()";
